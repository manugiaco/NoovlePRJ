<?xml version="1.0" encoding="UTF-8"?><database>
    <element label="Projects" max_length="40" name="x_snc_co2web_projects" type="collection">
        <element attributes="encode_utf8=false" label="Location" max_length="32" name="location" reference="cmn_location" type="reference"/>
        <element attributes="edge_encryption_enabled=true" display="true" label="Name" max_length="40" name="name" type="string"/>
        <element attributes="edge_encryption_enabled=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" type="string"/>
        <element attributes="encode_utf8=false" label="Objective" max_length="32" name="objective" reference="x_snc_co2web_objectives" type="reference"/>
        <element label="Progress" max_length="38" name="progress" type="percent_complete"/>
        <element attributes="edge_encryption_enabled=true" choice="3" default="planning" label="Status" max_length="40" name="status" type="choice">
            <choice>
                <element inactive_on_update="false" label="Ended" value="ended"/>
                <element inactive_on_update="false" label="Planning" value="planning"/>
                <element inactive_on_update="false" label="Started" value="started"/>
            </choice>
        </element>
        <index name="index">
            <element name="location"/>
        </index>
        <index name="index2">
            <element name="objective"/>
        </index>
    </element>
</database>
